{
  "name": "mfe-checkout",
  "version": "4.0.12",
  "engines": {
    "node": "18.x"
  },
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "export": "next build && next export",
    "start": "next start -p $PORT",
    "lint": "eslint ./ --ext .js,.jsx,.tsx,.ts",
    "lint:fix": "eslint ./ --ext .tsx,.ts --fix",
    "test": "playwright test",
    "test:debug": "PWDEBUG=1 playwright test",
    "serve": "serve -l tcp://localhost:$PORT/ -n -s out/",
    "dep:upgrade:major": "pnpm exec npm-check-updates -u",
    "dep:upgrade:minor": "pnpm dep:upgrade:major -t minor"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/commercelayer/mfe-checkout.git"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      [
        "@semantic-release/github",
        {
          "addReleases": "top"
        }
      ]
    ]
  },
  "author": {
    "name": "Matteo Alessani",
    "email": "matteo@commercelayer.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/commercelayer/mfe-checkout/issues"
  },
  "homepage": "https://github.com/commercelayer/mfe-checkout#readme",
  "keywords": [
    "reactjs",
    "nextjs",
    "jamstack",
    "headless",
    "ecommerce",
    "api",
    "checkout",
    "commercelayer"
  ],
  "dependencies": {
    "@commercelayer/react-components": "4.4.4",
    "@commercelayer/sdk": "^5.11.0",
    "@faker-js/faker": "^8.0.2",
    "@headlessui/react": "^1.7.16",
    "@next/bundle-analyzer": "^13.4.16",
    "@next/eslint-plugin-next": "^13.4.16",
    "@playwright/test": "^1.37.0",
    "@tailwindcss/forms": "^0.5.4",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@types/async-retry": "1.4.5",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.5.0",
    "@types/react": "^18.2.20",
    "@types/react-gtm-module": "^2.0.1",
    "@types/styled-components": "^5.1.26",
    "async-retry": "^1.3.3",
    "autoprefixer": "^10.4.15",
    "classnames": "^2.3.2",
    "dotenv": "^16.3.1",
    "i18next": "^23.4.4",
    "jsonwebtoken": "^9.0.1",
    "jwt-decode": "^3.1.2",
    "next": "^13.4.16",
    "next-build-id": "^3.0.0",
    "next-i18next": "^14.0.0",
    "postcss": "^8.4.27",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-gtm-module": "^2.0.11",
    "react-i18next": "^13.1.2",
    "react-router-dom": "^6.15.0",
    "styled-components": "^6.0.7",
    "tailwindcss": "^3.3.3",
    "twin.macro": "3.4.0",
    "typescript": "^5.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^10.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.0.4",
    "@semantic-release/npm": "^10.0.4",
    "@semantic-release/release-notes-generator": "^11.0.4",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "eslint-config-next": "^13.4.16",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-tailwind": "^0.2.1",
    "npm-check-updates": "^16.11.1",
    "prettier": "^3.0.1",
    "semantic-release": "^21.0.7",
    "serve": "^14.2.0",
    "stylelint": "^15.10",
    "stylelint-config-standard": "^34.0.0"
  },
  "resolutions": {
    "npm": "^8.18.0",
    "dset": "^3.1.2",
    "semver": ">=7.5.2",
    "word-break": ">=1.2.5"
  },
  "babelMacros": {
    "twin": {
      "preset": "styled-components"
    }
  },
  "cacheDirectories": [
    ".next/cache"
  ]
}
